
config aria2 'main'
	option user 'aria2'
	option bt_enable_lpd 'true'
	option enable_dht 'true'
	option follow_torrent 'true'
	option file_allocation 'none'
	option save_session_interval '30'
	list header ''
	option rpc_auth_method 'none'
	option dir '/mnt/sdb1/download'
	option overall_speed_limit '1'
	option max_overall_download_limit '3M'
	option max_overall_upload_limit '300K'
	option max_concurrent_downloads '30'
	option config_dir '/var/etc/aria2'
	list extra_settings 'max-resume-failure-tries=0'
	list extra_settings 'remote-time=true'
	list extra_settings 'save-session-interval=1'
	list extra_settings 'auto-save-interval=20'
	list extra_settings 'max-file-not-found=10'
	list extra_settings 'enable-peer-exchange=true'
	list extra_settings 'seed-time=10'
	list extra_settings 'seed-ratio=1.1'
	list extra_settings 'enable-dht6=true'
	list extra_settings 'dht-file-path6=/var/etc/aria2/dht6.dat.main'
	option enable_log 'true'
	option log_level 'info'
	option enabled '0'

